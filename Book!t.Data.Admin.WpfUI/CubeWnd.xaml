        <TextBox Height="23" HorizontalAlignment="Left" Margin="227,75,0,0" Name="textBox2" VerticalAlignment="Top" Width="106" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="227,114,0,0" Name="textBox3" VerticalAlignment="Top" Width="104" TextChanged="textBox3_TextChanged" />
        <Label Content="Label" Height="28" HorizontalAlignment="Left" Margin="227,249,0,0" Name="label2" VerticalAlignment="Top" Width="208" />
    </Grid>
</Window>
